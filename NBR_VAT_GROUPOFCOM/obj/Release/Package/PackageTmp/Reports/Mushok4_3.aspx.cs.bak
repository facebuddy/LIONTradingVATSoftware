    protected void btnReport_Click(object sender, EventArgs e)
    {
        if (drpDeclaretionNo.SelectedItem.Value.ToString() != "-99")
        {

            try
            {
                CompanyBLL CBll = new CompanyBLL();
                DataSet ds = CBll.getFullOrganizationInfo(Convert.ToInt32(Session["ORGANIZATION_ID"].ToString()));

                string strName = ds.Tables[0].Rows[0]["organization_name"].ToString();
                string strAddress = ds.Tables[0].Rows[0]["business_address"].ToString();
                string strRegNum = ds.Tables[0].Rows[0]["registration_no"].ToString();
                string strAreaCode = ds.Tables[0].Rows[0]["circle_code"].ToString();

                string strPhone = ds.Tables[0].Rows[0]["ba_phone"].ToString();
                string strFax = ds.Tables[0].Rows[0]["ba_fax"].ToString();

                Session["organization_name"] = strName;
                Session["business_address"] = strAddress;
                Session["registration_no"] = strRegNum;
                Session["area_code"] = strAreaCode;
                //Session["lblPriceDeclarationNumber"] = strDeclarationNum;
                Session["ba_phone"] = strPhone;
                Session["ba_fax"] = strFax;

                lblNibondhitoBektirName.Text = strName;
                lblThikana.Text = strAddress;
                lblNibondhonShonkha.Text = strRegNum;
                //lblElakaCode.Text = strAreaCode;
                //lblTelephoneNumber.Text = strPhone;
                //lblFaxNumber.Text = strFax;
                //lblThikana.Text = Session["ORGANIZATION_ADDRESS"].ToString();





                //DataTable dt = objPDBLL.getItemPriceNumber(Convert.ToInt32(drpDeclaretionNo.SelectedItem.Value.ToString()));
                //lblMulloGhoshonaNumber.Text = dt.Rows[0]["price_declaration_no"].ToString();
                //lblYear.Text = dt.Rows[0]["year"].ToString();

                pnlContents.Visible = true;
                btnPrint.Visible = true;

            }
            catch (Exception ex)
            {
                ReallySimpleLog.WriteLog(ex);
            }


            LoadFullReport();


        }
    }
